<section class="bg bg-navigation home--background">
  <div class="home--title">
    <a href="/">
      <div class="logo"></div>
      <div class="logo-title">RDocumentation</div>
    </a>
  </div>

  <ul class="navbar--navigation">
    <li><a href="/register" class="btn btn-primary">Create account</a></li>
    <li><a href="/login" class="btn btn-secondary">Sign in</a></li>
  </ul>

  <div class="home--cta">
    <h1>Search all <%= data.nbPackages %> CRAN and BiOC packages.</h1>
    <p><b><%= data.nbPackages %></b> packages, <b><%= data.nbPackageVersions %></b> package versions, <b><%= data.nbCollaborators %></b> collaborators</p>
    <div class="home--search">
      <form class="search" action="">
        <input type="text" placeholder="Search for packages, functions, authors, etc" autofocus>
        <div class="results">
          <ul class="packages">
            <li class="header">Packages</li>
          </ul>
          <ul class="topics">
            <li class="header">Topics</li>
          </ul>
        </div>
      </form>
    </div>
  </div>

  <p class="home--credits">Created by <a href="https://www.datacamp.com">DataCamp.com</a></p>
</section>
