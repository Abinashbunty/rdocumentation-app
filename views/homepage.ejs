<section class="bg bg-navigation home--background">
  <div class="home--title">
    <a href="/">
      <div class="logo"></div>
      <div class="logo-title">RDocumentation</div>
    </a>
  </div>

  <%- partial ('shared/_navigation.ejs') %>

  <div class="home--cta">
    <h1>Search all <%= data.nbPackages %> CRAN and BiOC packages.</h1>
    <p><b><%= data.nbPackages %></b> packages, <b><%= data.nbPackageVersions %></b> package versions, <b><%= data.nbCollaborators %></b> collaborators</p>
    <div class="home--search">
      <form class="search" action="/search">
        <input name="q" type="text" placeholder="Search for packages, functions, etc" autofocus>
        <button class="btn btn-primary" type="submit">Search</button>
        <div class="search--results"></div>
      </form>
    </div>
  </div>

  <p class="home--credits">Created by <a href="https://www.datacamp.com">DataCamp.com</a></p>
</section>
